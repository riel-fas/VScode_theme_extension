  "tokenColors": [
    {
      "scope": ["entity.name.type", "storage.type", "support.type", "meta.type", "variable.other"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#556B2F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF4500",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F5F5F5"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#90EE90"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#00CED1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#D3D3D3"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#00CED1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": "meta.class, entity.other.inherited-class, entity.name.type.class",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF4500"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#228B22"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#FFD700"
      }
    }
  ]
}
