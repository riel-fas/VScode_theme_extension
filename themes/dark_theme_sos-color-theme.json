{
  "name": "My Cyberpunk Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0a0a",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#111111",
    "editorCursor.foreground": "#4cd137",
    "editor.selectionBackground": "#2c2c2c",
    "editor.selectionHighlightBackground": "#1c1c1c",
    "editor.wordHighlightBackground": "#202020",
    "editorWhitespace.foreground": "#303030",
    "editorIndentGuide.background": "#333333",
    "editorIndentGuide.activeBackground": "#4cd137",
    "editorRuler.foreground": "#2e2e2e",
    "editorGutter.background": "#0a0a0a",
    "editorGutter.addedBackground": "#1b1b1b",
    "editorGutter.modifiedBackground": "#1b1b1b",
    "editorGutter.deletedBackground": "#1b1b1b",
    "activityBar.background": "#111111",
    "sideBar.background": "#111111",
    "statusBar.background": "#222222",
    "statusBar.foreground": "#ffffff",
    "titleBar.activeBackground": "#0a0a0a",
    "titleBar.activeForeground": "#ffffff",
    "button.background": "#9d4edd",
    "button.foreground": "#ffffff",
    "activityBar.border": "#18181a",
    "sideBar.border": "#18181a",
    "statusBar.border": "#18181a",
    "titleBar.border": "#18181a",
    "panel.border": "#18181a",
    "editorGroup.border": "#18181a",
    "tab.activeBorder": "#18181a",
    "tab.inactiveBorder": "#18181a",
    "focusBorder": "#18181a",
    "contrastActiveBorder": "#18181a",
    "contrastBorder": "#18181a",
    "icon.foreground": "#268a9e",
    "debugIcon.breakpointForeground": "#4cd137",
    "debugIcon.startForeground": "#4cd137"
  },
  "tokenColors": [
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.c",
        "storage.type",
        "storage.type.c",
        "storage.type.struct",
        "entity.name.struct",
        "entity.name.struct.c",
        "support.type",
        "support.class",
        "meta.type",
        "meta.type.identifier",
        "meta.type.struct",
        "variable.other",
        "variable.other.c"
      ],
      "settings": {
        "foreground": "#8a2be2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#ff4f97",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#e3ff00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#8be9fd",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f1fa8c",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#ff5555",
        "fontStyle": "normal"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#43afff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#f8f8f2"
      }
    }
  ]
}
