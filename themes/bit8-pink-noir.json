{
  "name": "8-Bit Pink Noir",
  "type": "dark",
  "colors": {
    "contrastActiveBorder": "#FF0",
    "contrastBorder": "#F0F",
    "focusBorder": "#F0F",
    "foreground": "#FFF",
    "descriptionForeground": "#FFF",
    "errorForeground": "#F00",
    "selection.background": "#F0F",
    "textLink.foreground": "#F0F",
    "editor.background": "#000",
    "editor.foreground": "#FFF",
    "editor.selectionBackground": "#F0F",
    "editor.selectionForeground": "#FFF",
    "editor.lineHighlightBackground": "#000",
    "editor.lineHighlightBorder": "#F0F",
    "editorCursor.foreground": "#F0F",
    "editorWhitespace.foreground": "#F0F",
    "editorLineNumber.foreground": "#FFF",
    "editorLineNumber.activeForeground": "#F0F",
    "panel.background": "#000",
    "panelTitle.activeForeground": "#F0F",
    "panelTitle.inactiveForeground": "#FFF",
    "panelSectionHeader.background": "#000",
    "activityBar.background": "#000",
    "activityBar.foreground": "#F0F",
    "activityBar.inactiveForeground": "#FFF",
    "activityBarBadge.background": "#000",
    "activityBarBadge.foreground": "#FFF",
    "sideBar.background": "#000",
    "sideBar.foreground": "#FFF",
    "input.background": "#000",
    "input.border": "#F0F",
    "input.foreground": "#FFF",
    "input.placeholderForeground": "#F0F",
    "inputOption.activeBackground": "#F0F",
    "inputOption.activeBorder": "#F0F",
    "inputOption.activeForeground": "#000",
    "dropdown.background": "#000",
    "dropdown.listBackground": "#000",
    "dropdown.foreground": "#FFF",
    "button.background": "#000",
    "button.foreground": "#FFF",
    "button.border": "#F0F",
    "badge.background": "#000",
    "badge.foreground": "#FFF",
    "tab.activeBackground": "#F0F",
    "tab.activeForeground": "#000",
    "tab.inactiveBackground": "#000",
    "tab.inactiveForeground": "#FFF",
    "tab.unfocusedActiveBackground": "#000",
    "tab.unfocusedActiveForeground": "#FFF",
    "tab.unfocusedInactiveBackground": "#000",
    "tab.unfocusedInactiveForeground": "#FFF",
    "diffEditor.removedTextBorder": "#F00",
    "diffEditor.insertedTextBorder": "#0F0",
    "editorWidget.background": "#000",
    "editorWidget.foreground": "#FFF",
    "minimap.findMatchHighlight": "#FF0",
    "minimap.selectionHighlight": "#F0F",
    "minimap.errorHighlight": "#F00",
    "minimap.warningHighlight": "#FF0",
    "minimap.background": "#000",
    "minimap.selectionOccurrenceHighlight": "#000",
    "scrollbarSlider.background": "#F0F7",
    "scrollbarSlider.hoverBackground": "#F0F7",
    "scrollbarSlider.activeBackground": "#F0FB",
    "terminal.background": "#000",
    "terminal.foreground": "#FFF",
    "terminal.ansiBlack": "#000",
    "terminal.ansiBlue": "#00F",
    "terminal.ansiCyan": "#0FF",
    "terminal.ansiGreen": "#0F0",
    "terminal.ansiMagenta": "#F0F",
    "terminal.ansiRed": "#F00",
    "terminal.ansiWhite": "#FFF",
    "terminal.ansiYellow": "#FF0",
    "terminal.ansiBrightBlack": "#000",
    "terminal.ansiBrightBlue": "#00F",
    "terminal.ansiBrightCyan": "#0FF",
    "terminal.ansiBrightGreen": "#0F0",
    "terminal.ansiBrightMagenta": "#F0F",
    "terminal.ansiBrightRed": "#F00",
    "terminal.ansiBrightWhite": "#FFF",
    "terminal.ansiBrightYellow": "#FF0",
    "terminal.selectionBackground": "#F0F",
    "statusBar.background": "#000",
    "statusBar.foreground": "#FFF",
    "settings.headerForeground": "#F0F",
    "settings.modifiedItemIndicator": "#F0F",
    "pickerGroup.border": "#F0F",
    "icon.foreground": "#F0F"
  },
  "tokenColors": [
    {
      "scope": [
        "entity.name.type",
        "storage.type",
        "support.type",
        "meta.type",
        "variable.other"
      ],
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6272a4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ffb86c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#8be9fd",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#8be9fd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": "meta.class, entity.other.inherited-class, entity.name.type.class",
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff5555"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#f1fa8c"
      }
    }
  ]
}
