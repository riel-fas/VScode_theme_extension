  "tokenColors": [
    {
      "scope": [
        "entity.name.type",
        "storage.type",
        "support.type",
        "meta.type",
        "variable.other"
      ],
      "settings": {
        "foreground": "#E91E63",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8D6E63",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF6B35",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFF8E1"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#81C784"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFD54F"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#FF8A65"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#4DD0E1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#BCAAA4"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#4DD0E1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": "meta.class, entity.other.inherited-class, entity.name.type.class",
      "settings": {
        "foreground": "#E91E63"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF5722"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FF6B35"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFD54F"
      }
    }
  ]
